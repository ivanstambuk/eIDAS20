{
  "name": "docs-portal",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "validate:config": "node scripts/validate-documents.js",
    "validate:css": "node scripts/validate-css-classes.js",
    "validate:rca": "node scripts/validate-rca.js",
    "validate:vcq": "node scripts/validate-vcq.js",
    "validate:vcq-arf": "node scripts/validate-vcq-arf.js",
    "validate:subtitles": "node scripts/validate-subtitles.js",
    "validate:annex": "node scripts/validate-annex-content.js",
    "validate:ci": "npm run validate:config && npm run validate:css && npm run validate:rca && npm run validate:vcq && npm run validate:subtitles && npm run validate:annex",
    "build": "npm run validate:config && npm run validate:css && npm run validate:rca && npm run validate:vcq && node scripts/build-content.js && npm run validate:subtitles && node scripts/build-terminology.js && node scripts/build-rca.js && node scripts/build-vcq.js && node scripts/import-arf.js && node scripts/build-search-index.js && node scripts/build-embeddings.js && vite build",
    "build:documents": "node scripts/build-content.js",
    "build:terminology": "node scripts/build-terminology.js && npm run build:search && npm run build:documents",
    "build:all-content": "npm run build:terminology && npm run build:search",
    "build:rca": "node scripts/build-rca.js",
    "build:vcq": "node scripts/build-vcq.js",
    "build:vcq-clarifications": "node scripts/build-vcq-clarifications.js",
    "build:search": "node scripts/build-search-index.js",
    "build:embeddings": "node scripts/build-embeddings.js",
    "build:citations": "node scripts/build-citations.js",
    "build:arf": "node scripts/import-arf.js",
    "normalize:regulation-ids": "node scripts/normalize-regulation-ids.js",
    "extract:articles": "node scripts/extract-article-excerpts.js",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@mlc-ai/web-llm": "^0.2.80",
    "@orama/orama": "^3.1.18",
    "@xenova/transformers": "^2.17.2",
    "ajv": "^8.17.1",
    "js-yaml": "^4.1.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.12.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "xlsx-js-style": "^1.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "unist-util-visit-parents": "^6.0.2",
    "vite": "^7.2.4"
  }
}